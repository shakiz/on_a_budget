<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="expensePlanner9VM" type="com.finance.onabudget.app.modules.expenseplanner9.data.viewmodel.ExpensePlanner9VM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnmegaphone"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowmegaphone"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStyleteal_400">
                <ImageView android:id="@+id/imageMegaphone"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginBottom="@dimen/_30pxv"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_megaphone"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_megaphone"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEditdailyexpe"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_23pxv"
                    android:layout_marginStart="@dimen/_60pxh"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_weight="0.83"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{expensePlanner9VM.expensePlanner9Model.txtEditdailyexpe}"
                    style="@style/txtRobotoromansemibold20" tools:text="@string/msg_edit_daily_expe"/>
                <ImageView android:id="@+id/imageVector"
                    android:layout_height="@dimen/_19pxh"
                    android:layout_marginBottom="@dimen/_25pxv"
                    android:layout_marginEnd="@dimen/_21pxh"
                    android:layout_marginStart="@dimen/_62pxh"
                    android:layout_marginTop="@dimen/_21pxv"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="start"
            android:id="@+id/linear28Expenseplan"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumndescription"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyleteal_401">
                <TextView android:gravity="center_horizontal"
                    android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_31pxh"
                    android:layout_marginStart="@dimen/_31pxh"
                    android:layout_marginTop="@dimen/_34pxv"
                    android:layout_width="match_parent"
                    android:text="@{expensePlanner9VM.expensePlanner9Model.txtDescription}"
                    style="@style/txtRobotoromansemibold14" tools:text="@string/msg_congratulations12"/>
                <ImageView android:id="@+id/image55221"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_180pxh"
                    android:layout_marginEnd="@dimen/_31pxh"
                    android:layout_marginStart="@dimen/_31pxh"
                    android:layout_marginTop="@dimen/_22pxv"
                    android:layout_width="@dimen/_228pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_55221}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_55221"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnBack"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_11pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_11pxv"
                android:singleLine="true"
                android:text="@string/lbl_back"
                android:textAllCaps="false"
                style="@style/btnSolidRounded" tools:text="@string/lbl_back"/>
            <FrameLayout android:id="@+id/frameStackgroupeightysix"
                android:layout_height="@dimen/_134pxv"
                android:layout_marginTop="@dimen/_238pxv" android:layout_width="match_parent">
                <View
                    android:background="@drawable/rectangle_bg_teal_400_radius_10"
                    android:id="@+id/viewGroupEightySix"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_115pxv"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_360pxh"/>
                <LinearLayout android:id="@+id/linearRowplus"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_21pxh"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="end"
                        android:id="@+id/linearColumnplus"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.44"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageButton
                            android:background="@drawable/rectangle_bg_white_a700_radius_30"
                            android:id="@+id/btnPlus"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_60pxh"
                            android:layout_marginEnd="@dimen/_76pxh"
                            android:layout_width="@dimen/_60pxh"
                            android:paddingHorizontal="@dimen/_22pxh"
                            android:paddingVertical="@dimen/_22pxv"
                            android:scaleType="fitCenter"
                            android:src="@drawable/img_plus" tools:ignore="ContentDescription"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtExpense"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_80pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{expensePlanner9VM.expensePlanner9Model.txtExpense}"
                            style="@style/txtRobotoromanregular12" tools:text="@string/lbl_expense"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearColumngroup"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_40pxv"
                        android:layout_weight="0.42"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageGroup"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_18pxh"
                            android:layout_marginEnd="@dimen/_15pxh"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_group"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_group"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtAnalytics"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{expensePlanner9VM.expensePlanner9Model.txtAnalytics}"
                            style="@style/txtRobotoromanregular12" tools:text="@string/lbl_analytics"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearColumnlaptop"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_39pxv"
                        android:layout_weight="0.13"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageLaptop"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginEnd="@dimen/_12pxh"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_width="@dimen/_14pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_laptop"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_laptop"/>
                        <TextView android:ellipsize="end"
                            android:gravity="start"
                            android:id="@+id/txtPlanner"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{expensePlanner9VM.expensePlanner9Model.txtPlanner}"
                            style="@style/txtRobotoromanregular12" tools:text="@string/lbl_planner"/>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStyleblack_900">
            <LinearLayout android:id="@+id/linearRowrectanglethirteen"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_13pxv"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <View android:background="@color/white_A700"
                    android:id="@+id/viewRectangleThirteen"
                    android:layout_height="@dimen/_14pxh" android:layout_width="@dimen/_14pxh"/>
                <LinearLayout android:id="@+id/linearRowgrouptwentyfour"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.93"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageGroupTwentyFour"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_group24"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_group24"/>
                    <ImageView android:id="@+id/imageRectangleFourteen"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginEnd="@dimen/_109pxh"
                        android:layout_marginStart="@dimen/_50pxh"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_rectangle14"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle14"/>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
