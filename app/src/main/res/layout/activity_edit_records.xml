<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="editRecordsVM" type="com.finance.onabudget.app.modules.editrecords.data.viewmodel.EditRecordsVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linear41editrecords"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnmegaphone"
            android:layout_height="0dp" android:layout_weight="1.0"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStackmegaphone"
                android:layout_height="@dimen/_379pxv" android:layout_width="match_parent">
                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbarToolbar"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    app:contentInsetStart="0dp" style="@style/groupStyleteal_400">
                    <LinearLayout android:id="@+id/linearRowmegaphone"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageMegaphone"
                            android:layout_height="@dimen/_12pxh"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_megaphone"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_megaphone"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtEditRecords"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_85pxh"
                            android:layout_weight="0.77"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{editRecordsVM.editRecordsModel.txtEditRecords}"
                            style="@style/txtRobotoromansemibold20" tools:text="@string/lbl_edit_records"/>
                        <ImageView android:id="@+id/imageVector"
                            android:layout_height="@dimen/_19pxh"
                            android:layout_marginStart="@dimen/_86pxh"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <View
                    android:background="@drawable/rectangle_bg_teal_400_border_white_a700_6d"
                    android:id="@+id/viewRectangleSixteen"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_274pxv"
                    android:layout_marginBottom="@dimen/_43pxv"
                    android:layout_marginTop="@dimen/_43pxv" android:layout_width="@dimen/_360pxh"/>
                <LinearLayout android:id="@+id/linearColumnc"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_21pxh"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                    <LinearLayout android:id="@+id/linearRowc"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_41pxh"
                        android:layout_marginStart="@dimen/_41pxh"
                        android:layout_marginTop="@dimen/_23pxv"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylewhite_1">
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnC"
                            android:layout_height="@dimen/_40pxv"
                            android:layout_width="@dimen/_54pxh"
                            android:lineSpacingExtra="@dimen/_4pxh"
                            android:paddingBottom="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_19pxh"
                            android:paddingStart="@dimen/_21pxh"
                            android:paddingTop="@dimen/_8pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_c"
                            android:textAllCaps="false"
                            style="@style/btnSolidRoundedOutline" tools:text="@string/lbl_c"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtThreeHundred"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_7pxv"
                            android:layout_marginEnd="@dimen/_11pxh"
                            android:layout_marginStart="@dimen/_136pxh"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_weight="0.39"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_4pxh"
                            android:singleLine="true"
                            android:text="@{editRecordsVM.editRecordsModel.txtThreeHundred}"
                            style="@style/txtRobotoromansemibold20_1" tools:text="@string/lbl_300"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnotpview"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_23pxv"
                        android:layout_marginEnd="@dimen/_41pxh"
                        android:layout_marginStart="@dimen/_41pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <com.mukesh.OtpView
                            android:focusableInTouchMode="true"
                            android:id="@+id/otpViewOtpview"
                            android:inputType="number"
                            android:itemBackground="@color/teal_400_2b"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:textColor="@color/gray_700"
                            android:textSize="@dimen/_20pxh"
                            app:OtpItemCount="4"
                            app:OtpItemHeight="@dimen/_40pxv"
                            app:OtpItemSpacing="@dimen/_7pxh"
                            app:OtpItemWidth="@dimen/_54pxh"
                            app:OtpLineColor="@color/gray_300" app:OtpViewType="rectangle"/>
                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/recyclerListgroupeightyfour"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                            itemSpace="@{@dimen/_7pxh}"
                            tools:itemCount="3" tools:listitem="@layout/row_listgroupeightyfour"/>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
            <FrameLayout android:id="@+id/frameStackgroupfortyseven"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_43pxv"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="@dimen/_318pxh" style="@style/groupStylewhite_2">
                <ProgressBar
                    android:id="@+id/progressBarGroupFortySeven"
                    android:indeterminate="false"
                    android:layout_gravity="bottom|end"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:max="100" android:minHeight="@dimen/_8pxv"
                    android:progress="9"
                    android:progressDrawable="@drawable/layer_list_bg_teal_500_26_radius_4" style="@style/Widget.AppCompat.ProgressBar.Horizontal"/>
                <LinearLayout android:id="@+id/linearRowthreehundredOne"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearRowsettings"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.93"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageButton
                            android:background="@drawable/rectangle_bg_teal_500_26_radius_15"
                            android:id="@+id/btnSettings"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_30pxh"
                            android:paddingHorizontal="@dimen/_3pxh"
                            android:paddingVertical="@dimen/_3pxv"
                            android:scaleType="fitCenter"
                            android:src="@drawable/img_settings" tools:ignore="ContentDescription"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLanguage"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_7pxv"
                            android:layout_marginStart="@dimen/_6pxh"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_weight="0.18"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{editRecordsVM.editRecordsModel.txtLanguage}"
                            style="@style/txtRobotoromanmedium12" tools:text="@string/lbl_baby2"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txt5of100000"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_21pxv"
                            android:layout_marginStart="@dimen/_25pxh"
                            android:layout_weight="0.62"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_2pxh"
                            android:singleLine="true"
                            android:text="@{editRecordsVM.editRecordsModel.txt5of100000}"
                            style="@style/txtRobotoromanlight8" tools:text="@string/msg_5_of_1_00_000"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtThreeHundredOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_20pxv"
                        android:layout_marginStart="@dimen/_101pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.07"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_2pxh"
                        android:singleLine="true"
                        android:text="@{editRecordsVM.editRecordsModel.txtThreeHundredOne}"
                        style="@style/txtRobotoromanmedium8" tools:text="@string/lbl_300"/>
                </LinearLayout>
            </FrameLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnNext"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_11pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_11pxv"
                android:singleLine="true"
                android:text="@string/lbl_next"
                android:textAllCaps="false"
                style="@style/btnSolidRounded" tools:text="@string/lbl_next"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStyleteal_400cornerRadius">
            <LinearLayout android:id="@+id/linearColumngrouptwentyfive"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearRowgrid"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearColumngrid"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageGrid"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_width="@dimen/_14pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_grid"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_grid"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtExpense"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{editRecordsVM.editRecordsModel.txtExpense}"
                            style="@style/txtRobotoromanregular12" tools:text="@string/lbl_expense"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearColumngroup"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageGroup"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_18pxh"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_group"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_group"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtAnalytics"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{editRecordsVM.editRecordsModel.txtAnalytics}"
                            style="@style/txtRobotoromanregular12" tools:text="@string/lbl_analytics"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearColumnlaptop"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageLaptop"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_width="@dimen/_14pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_laptop"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_laptop"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtPlanner"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{editRecordsVM.editRecordsModel.txtPlanner}"
                            style="@style/txtRobotoromanregular12" tools:text="@string/lbl_planner"/>
                    </LinearLayout>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameSafebottom"
                    android:layout_height="@dimen/_40pxv"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent" style="@style/groupStyleblack_900">
                    <ImageView android:id="@+id/imageGroupTwentyFive"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginBottom="@dimen/_13pxv"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="@dimen/_142pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_group25"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_group25"/>
                </FrameLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
